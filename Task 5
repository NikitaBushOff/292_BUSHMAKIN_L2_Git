function primeFactors(n) {
    let result = '';
    for (let i = 2; n > 1; i++) {
        let count = 0;
        while (n % i === 0) {
            count++;
            n /= i;
        }
        if (count) {
            result += count === 1 ? `(${i})` : `(${i}**${count})`;
        }
    }
    return result;
}
